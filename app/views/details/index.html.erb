<h1>Listing details</h1>

<table>
  <tr>
    <th>Numac</th>
    <th>Vehicule</th>
    <th>Misecirc</th>
    <th>Tues</th>
    <th>Hospitalises</th>
    <th>Legers</th>
    <th>Indemnes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @details.each do |detail| %>
  <tr>
    <td><%= detail.numac %></td>
    <td><%= detail.vehicule %></td>
    <td><%= detail.misecirc %></td>
    <td><%= detail.tues %></td>
    <td><%= detail.hospitalises %></td>
    <td><%= detail.legers %></td>
    <td><%= detail.indemnes %></td>
    <td><%= link_to 'Show', detail %></td>
    <td><%= link_to 'Edit', edit_detail_path(detail) %></td>
    <td><%= link_to 'Destroy', detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

